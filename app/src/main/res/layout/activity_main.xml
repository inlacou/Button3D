<?xml version="1.0" encoding="utf-8"?>
<ScrollView
	android:id="@+id/scroll"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout
		android:id="@+id/activity_main"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:paddingTop="@dimen/activity_vertical_margin"
		android:paddingBottom="@dimen/activity_vertical_margin"
		android:paddingLeft="@dimen/activity_horizontal_margin"
		android:paddingRight="@dimen/activity_horizontal_margin"
		android:orientation="vertical"
		tools:context="libraries.inlacou.com.button3d.MainActivity">
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_match
			android:id="@+id/match"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me!"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_wrap
			android:id="@+id/wrap"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:textSize="24sp"
			app:textColor="@color/colorAccent"
			app:text="gGpPqQ!"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_wrap
			android:id="@+id/wrap2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:drawableLeft="@drawable/documentacion_section"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me!"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_wrap
			android:id="@+id/wrap3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me!"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_wrap
			android:id="@+id/wrap4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:drawableLeft="@drawable/documentacion_section"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me!"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_wrap
			android:id="@+id/wrap5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:drawablePadding="@dimen/threedimensionalbutton_general_all"
			app:drawableLeft="@drawable/documentacion_section"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me! Butterknife"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_match
			android:id="@+id/wrap6"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:drawablePadding="@dimen/threedimensionalbutton_general_all"
			app:drawableLeft="@drawable/documentacion_section"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me! call match"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_match
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			app:drawableLeft="@drawable/documentacion_section"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="Click me!"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_match
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:enabled="false"
			app:textColor="@color/colorAccent"
			app:text="I am disabled"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_match
			android:id="@+id/wrap9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="I am disabled"/>
		<libraries.inlacou.com.threedimensionalbutton.ThreeDimensionalButton_match
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:theme="@style/ButtonTheme"
			app:backgroundNormal="@drawable/my_rectangle_bordered_grey_dark_shadowed"
			app:backgroundPressed="@drawable/my_rectangle_bordered_grey_dark"
			app:textSize="@dimen/text_size_big"
			app:textColor="@color/colorAccent"
			app:text="I am disabled"/>
	</LinearLayout>
</ScrollView>
